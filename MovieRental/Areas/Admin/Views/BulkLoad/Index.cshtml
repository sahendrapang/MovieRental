
@{
    ViewBag.Title = "Index";
}

<h2>Movie Bulk Loader</h2>
Enter List of Movies (one line per movies) to load
@using (Html.BeginForm()) 
{
    @Html.TextArea("MovieList")
    <p>Default Rental Rate:<br />@Html.TextBox("defaultRentalRate")</p>
    <p>Default Quantity:<br/>@Html.TextBox("defaultQty")</p>
    <p><input type ="submit" value = "Submit"</p>
}

